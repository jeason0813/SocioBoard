<script type="text/javascript">

    function bindmanageSchdlServices() {
        $('#SchedulingDataTable').html('<center><img src="../Themes/Socioboard/Contents/img/admin/loading.gif" /></center>');
        $.ajax({
            type: 'POST',
            url: '../Services/LoadSheduling',
            success: function (msg) {
                //alert(msg);
                $('#SchedulingDataTable').html(msg);
                $('#SchedulingDataTable').dataTable();
            },
        });
    }
