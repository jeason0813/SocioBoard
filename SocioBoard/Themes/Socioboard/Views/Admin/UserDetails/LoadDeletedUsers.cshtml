@using Domain.Socioboard.Domain;
<div id="page-wrapper">
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Deleted Users</h1>
        </div>

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Deleted Users Table
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="row"></div>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" cellspacing="0" width="100%" id="ManageUserDataTable">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Account Type</th>
                                    <th>Create Date</th>
                                    <th>Email Id</th>
                                    <th>User Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (User item in (List<User>)Model)
                                {
        
                                    <tr class="odd gradeX">
                                        @*<td onclick="EditUserDetails('@item.Id');"><i class="fa fa-pencil fa-fw" style="cursor: pointer"></i></td>
                                        <td onclick="DeleteUserDetails('@item.Id');"><i class="fa fa-trash fa-fw" style="cursor: pointer"></i></td>*@
                                        <td>@item.UserName</td>
                                        <td>@item.AccountType</td>
                                        <td>@item.CreateDate</td>
                                        <td>@item.EmailId</td>
                                        <td>Deleted</td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->

    </div>
    <!-- /.row -->


</div>
