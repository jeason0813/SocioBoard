@using Domain.Socioboard.Domain;
@{Layout = null;}
<thead>
    <tr>
        <th>Edit</th>
        <th>Ads</th>
        <th>Url or Script</th>
        <th>Entry Date</th>
        <th>Expiry  Date</th>
        <th>Status</th>
    </tr>
</thead>

<tbody>
    @foreach (Domain.Socioboard.Domain.Ads item in (List<Ads>)Model)
    {
        
        <tr class="odd gradeX">
            <td onclick="EditAdvertisementDetails('@item.Id');"><i class="fa fa-pencil fa-fw" style="cursor:pointer"></i></td>
            <td>@item.Advertisment</td>
            <td>@item.ImageUrl</td>
            <td>@item.EntryDate</td>
            <td>@item.ExpiryDate</td>
            <td>@item.Status</td>
        </tr>
    }
</tbody>


