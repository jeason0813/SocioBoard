@using Domain.Socioboard.Domain;
@{Layout = null;}
<thead>
    <tr>
        <th></th>
        <th>Coupons</th>
        <th>Entry Date</th>
        <th>Expiry  Date</th>
        <th>Status</th>
        <th>Discount</th>
    </tr>
</thead>

<tbody>
    @foreach (Domain.Socioboard.Domain.Coupon item in (List<Coupon>)Model)
    {
        
        <tr class="odd gradeX">
            <td onclick="EditCoupontDetails('@item.Id');"><i class="fa fa-pencil fa-fw" style="cursor:pointer"></i></td>
            <td>@item.CouponCode</td>
            <td>@item.EntryCouponDate</td>
            <td>@item.ExpCouponDate</td>
            <td>@item.Status</td>
            <td>@item.Discount</td>
        </tr>
    }
</tbody>


