@using Domain.Socioboard.Domain;
@{
    string path = @System.Configuration.ConfigurationManager.AppSettings["domain"];
}
@foreach (var item in (Dictionary<TeamMemberProfile, object>)Model)
{
    if (item.Key.ProfileType == "facebook" || item.Key.ProfileType=="facebook_page")
    {
        FacebookAccount objFacebookAccount = (FacebookAccount)item.Value;
        string src = "http://graph.facebook.com/" +objFacebookAccount.FbUserId + "/picture?type=small" ;
        if(objFacebookAccount.AccessToken!="")
        {
            
    <li onclick="bindfrofiles('facebook','@objFacebookAccount.FbUserName','@objFacebookAccount.FbUserId')">
        <a href="#"> <img  src="@src" class=" headerProfilePic" title="@objFacebookAccount.FbUserName" />
            <i class="fa fa-facebook fa-fw"></i>@objFacebookAccount.FbUserName
                                                    </a>
    </li>
}
    }
    if (item.Key.ProfileType == "twitter")
    {
        TwitterAccount objTwitterAccount = (TwitterAccount)item.Value;
    <li onclick="bindfrofiles('twitter','@objTwitterAccount.TwitterScreenName','@objTwitterAccount.TwitterUserId')">
        <a href="#"> <img src="@objTwitterAccount.ProfileImageUrl" class=" headerProfilePic" title="@objTwitterAccount.TwitterScreenName">
            <i class="fa fa-twitter  fa-fw"></i>@objTwitterAccount.TwitterScreenName
                                                    </a>
    </li>
    }
    if (item.Key.ProfileType == "linkedin")
    {
        LinkedInAccount objLinkedInAccount = (LinkedInAccount)item.Value;
    <li onclick="bindfrofiles('linkedin','@objLinkedInAccount.LinkedinUserName','@objLinkedInAccount.LinkedinUserId')">
        <a href="#"> <img src="@objLinkedInAccount.ProfileImageUrl" class=" headerProfilePic" title="@objLinkedInAccount.LinkedinUserName">
            <i class="fa fa-linkedin fa-fw"></i>@objLinkedInAccount.LinkedinUserName
                                                    </a>
    </li>
    }
    if (item.Key.ProfileType == "instagram")
    {
        InstagramAccount objInstagramAccount = (InstagramAccount)item.Value;

    }
    if (item.Key.ProfileType == "tumblr")
    {
        TumblrAccount objTumblrAccount = (TumblrAccount)item.Value;
        string tumblruserimage = "http://api.tumblr.com/v2/blog/" + objTumblrAccount.tblrUserName + ".tumblr.com/avatar";
    <li onclick="bindfrofiles('tumblr','@objTumblrAccount.tblrUserName','@objTumblrAccount.tblrUserName')">
        <a href="#"> <img src="@tumblruserimage" title="@objTumblrAccount.tblrUserName" class="headerProfilePic">
            <i class="fa fa-tumblr fa-fw"></i>@objTumblrAccount.tblrUserName
                                                    </a>
    </li>
    }
    if (item.Key.ProfileType == "youtube")
    {
        YoutubeAccount objYoutubeAccount = (YoutubeAccount)item.Value;

    }
}

