<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="RssFeeds" table="RssFeeds" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="Message">
      <column name="Message" />
    </property>
    <property name="Duration">
      <column name="Duration" />
    </property>
    <property name="ProfileScreenName">
      <column name="ProfileScreenName"  />
    </property>
    <property name="FeedUrl">
      <column name="FeedUrl"  />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate"  />
    </property>
    <property name="Title">
      <column name="Title"  />
    </property>
    <property name="PublishingDate">
      <column name="PublishingDate"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>
    <property name="Link">
      <column name="Link"  />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>
    <property name="Profiletype">
      <column name="Profiletype"  />
    </property>

    <property name="UserId">
      <column name="UserId" />
    </property>
  </class>
</hibernate-mapping>
