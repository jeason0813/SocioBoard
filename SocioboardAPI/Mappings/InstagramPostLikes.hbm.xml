<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="InstagramPostLikes" table="instagrampostlikes" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="Profile_Id">
      <column name="Profile_Id" />
    </property>
    <property name="Feed_Id">
      <column name="Feed_Id"  />
    </property>
    <property name="Liked_By_Id">
      <column name="Liked_By_Id"  />
    </property>
    <property name="Liked_By_Name">
      <column name="Liked_By_Name"  />
    </property>   
     
     <property name="Feed_Type">
      <column name="Feed_Type"  />
    </property>
    <property name="Created_Date">
      <column name="Created_Date"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>
  
  </class>
</hibernate-mapping>