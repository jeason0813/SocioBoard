<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="GoogleplusComments" table="GoogleplusComments" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="GplusUserId">
      <column name="GplusUserId" />
    </property>
    <property name="FeedId">
      <column name="FeedId" />
    </property>
    <property name="CommentId">
      <column name="CommentId"  />
    </property>
    <property name="Comment">
      <column name="Comment"  />
    </property>
    <property name="FromId">
      <column name="FromId" />
    </property>
    <property name="FromName">
      <column name="FromName" />
    </property>
    <property name="FromImageUrl">
      <column name="FromImageUrl"  />
    </property>
    <property name="FromUrl">
      <column name="FromUrl"  />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate"  />
    </property>
  </class>
</hibernate-mapping>
