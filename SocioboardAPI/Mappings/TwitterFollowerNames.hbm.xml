<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="TwitterFollowerNames" table="twitterfollowernames" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="TwitterProfileId">
      <column name="TwitterProfileId" />
    </property>
    <property name="FollowerId">
      <column name="FollowerId"  />
    </property>
    <property name="Followerscrname">
      <column name="Followerscrname"  />
    </property>
    <property name="Name">
      <column name="Name"  />
    </property>

  </class>
</hibernate-mapping>