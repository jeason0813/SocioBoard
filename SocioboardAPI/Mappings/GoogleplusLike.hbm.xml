<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="GoogleplusLike" table="GoogleplusLike" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="ProfileId">
      <column name="ProfileId" />
    </property>
    <property name="FromId">
      <column name="FromId" />
    </property>
    <property name="FromName">
      <column name="FromName"  />
    </property>
    <property name="FromImageUrl">
      <column name="FromImageUrl"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>
    <property name="FeedId">
      <column name="FeedId"  />
    </property>
    <property name="FromUrl">
      <column name="FromUrl"  />
    </property>
    <property name="CreateDate">
      <column name="CreateDate"  />
    </property>
  </class>
</hibernate-mapping>